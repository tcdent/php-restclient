{
    "name": "tcdent/php-restclient", 
    "version": "0.2", 
    "description": "A generic REST API client for PHP", 
    "type": "library", 
    "keywords": ["REST", "api", "client", "curl", "JSON", "XML"], 
    "homepage": "http://github.com/tcdent/php-restclient", 
    "license": "MIT", 
    "authors": [
        {
            "name": "Travis Dent", 
            "role": "Developer", 
            "email": "tcdent@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/tcdent/php-restclient/issues"
    }, 
    "require": {
        "php": ">=8.3", 
        "ext-curl": "*", 
        "ext-json": "*"
    }, 
    "require-dev": {
        "php": ">=8.3", 
        "phpunit/phpunit": ">=9.6"
    }, 
    "autoload": {
        "classmap": ["src/RestClient.php"], 
        "files": [
            "src/RestClient/init.php", 
            "src/RestClient/Exception.php"
        ], 
        "psr-4": {
            "RestClient\\": "src/RestClient"
        }
    }
}
